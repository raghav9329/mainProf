package Test;

public class mainrun{

	public static void main(String[] args) {
		System.out.println( "is 5 prime ");

		
		
	}
	public boolean isPrime(int n){
		if (n < 2 ){
			return false;
		}
		
		for ( int i = 2; i <= Math.sqrt(n) ; i++ ) {
			if (n % i == 0){
				return false;
			}
		}
	return true;
	}
}
