  "description": "protractor_Demo",
  "license": "none",
  "readme": "",
  "scripts": {
    "****************************Build****************************": "",
    "build": "gulp build",
    "clean": "gulp clean",
    "****************************Unit Tests****************************": "",
    "test": "gulp test",
    "coverage": "gulp test --withCoverage --browsers=Chrome",
    "****************************Integration Tests****************************": "",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor",
    "****************************Tools****************************": "",
    "start": "npm run deploy && http-server -a localhost -p 9000 -c-1 -o http://localhost/",
    "watch": "gulp watch",
    "****************************Deployment****************************": "",
    "deploy": "gulp deploy",
    "vagrant": "gulp build --vagrant=true"
  },
