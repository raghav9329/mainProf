  "jshintConfig": {
    "globals": {
      "envConfig": true,
      "expect": true,
      "inject": true,
      "$": true,
      "$$": true,
      "browser": true,
      "protractor": true,
      "angular": true,
      "element": true,
      "by": true
    },
    "esnext": true,
    "browser": true,
    "camelcase": true,
    "curly": true,
    "devel": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "jasmine": true,
    "maxlen": 120,
    "newcap": true,
    "node": true,
    "mocha": true,
    "nonew": true,
    "strict": false,
    "undef": true,
    "unused": "vars",
    "overrides": {
      "test/**/*.js$": {
        "globals": {
          "$": true
