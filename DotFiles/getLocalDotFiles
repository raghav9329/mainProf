#!/bin/bash
echo "copying bash_profile, bashrc, dircolors"
echo "        getconfig, sentientrc, svbrc, viminfo"
echo "        vimrc, theColors, vimrc_MasterCopy"
sleep 4

cp ~/.atkmhrc .
cp ~/.bash_eternal_history .
cp ~/.bash_history .
cp ~/.bash_profile .
cp ~/.bashrc .
cp ~/.dircolors .
cp ~/.gitconfig .
cp ~/.gitignore .
cp ~/.gitignore_global .
cp ~/.hgignore_global .
cp ~/.lesshst .
cp ~/.sentientrc .
cp ~/.svbrc .
cp ~/.viminfo .
cp ~/.vimrc .
cp ~/theColors .
cp ~/vimrc__MasterCopy .

echo "Done. now doing a git status to check what's changed "
sleep 3
git status
